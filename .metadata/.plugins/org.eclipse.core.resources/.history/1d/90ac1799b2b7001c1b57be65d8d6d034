package com.revature.p2.myp2.ctrl;

public class UserTrans {
    private double amount;

    public UserTrans() {
    }

    public UserTrans(double amount) {
        this.amount = amount;
    }

    public double getAmount() {
        return amount;
    }

    public void setAmount(double amount) {
        this.amount = amount;
    }
}
